{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"$app/*": [".svelte/dev/runtime/app/*", ".svelte/build/runtime/app/*"],
			"$lib/*": ["src/lib/*"],
			"$lib": ["src/lib"],
			"$routes": ["src/routes"],
			"$routes/*": ["src/routes/*"],
			"$src/*": ["src/*"]
		}
	},
	"extends": "./.svelte-kit/tsconfig.json",
	"include": ["src/**/*.d.ts", "src/**/*.js", "src/**/*.svelte"]
}
